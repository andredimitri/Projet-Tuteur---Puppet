;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA    dns.ptut.grid5000.fr. root.ptut.grid5000.fr. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS     dns. 

