;
;BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.ptut.grid500.fr. root.localhost. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.ptut.grid5000.fr

